@model IEnumerable<Teacher>
@{
    ViewData["Title"] = "Teacher's Page";
}

<h1>Список учителей</h1>

<table class="table table-bordered table-striped"style="width:100%" >
    <thead>
        <tr>
            <th>
                Имя
            </th>
            <th>
                Фамилия
            </th>
            <th>
                Отчество
            </th>
             <th>
                Предмет
            </th>
            
             <tbody>
                 @foreach(var obj in Model){
                     <tr>
                         <td width=25%>@obj.Name</td>
                          <td width=25%>@obj.LastName</td>
                          <td width=25%>@obj.Patronymic</td>
                          <td width =25%>@obj.Discipline.Name</td>
                         
                     </tr>
                 }
             </tbody>
            
        </tr>
    </thead>
</table>